      ],
      "include": [
        "src/physfs.h"
      ]
    }
  },
  "android": {
    "cmake": {
      "args": [
        "-DPHYSFS_BUILD_SHARED=ON",
        "-DPHYSFS_BUILD_STATIC=OFF",
        "-DPHYSFS_INSTAL=ON",
        "-DPHYSFS_INSTALL_MAN=OFF",
        "-DPHYSFS_BUILD_DOCS=OFF",
        "-DPHYSFS_BUILD_TEST=OFF"
      ]
    },
    "name": "PhysFS",
    "modules": {
      "PhysFS-shared": {
        "type": "library",
        "library": "lib/libphysfs.so",
        "includes": {
          ".": ["include/physfs.h"]
        }
      },
      "PhysFS": {
        "type": "interface",
        "export-libraries": [":PhysFS-shared"]
