      ]
    }
  },
  "msvc": {
    "cmake": {
      "archs": [
        "x86",
        "x64",
        "arm64"
      ],
      "args": [
        "-DPHYSFS_BUILD_SHARED=ON",
        "-DPHYSFS_BUILD_STATIC=OFF",
        "-DPHYSFS_INSTAL=ON",
        "-DPHYSFS_INSTALL_MAN=OFF",
        "-DPHYSFS_BUILD_DOCS=OFF",
        "-DPHYSFS_BUILD_TEST=OFF"
      ],
      "files-lib": {
        "": [
          "bin/physfs.dll"
        ]
      },
      "files-devel": {
