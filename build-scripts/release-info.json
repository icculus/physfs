      "test/test_physfs.c",
      "extras/physfssdl3.h"
    ]
  },
  "mingw": {
    "cmake": {
      "archs": ["x86", "x64"],
      "args": [
        "-DPHYSFS_BUILD_SHARED=ON",
        "-DPHYSFS_BUILD_STATIC=OFF",
