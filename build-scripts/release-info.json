    "re_major": "^#define PHYSFS_VER_MAJOR\\s+([0-9]+)$",
    "re_minor": "^#define PHYSFS_VER_MINOR\\s+([0-9]+)$",
    "re_micro": "^#define PHYSFS_VER_PATCH\\s+([0-9]+)$"
